{
  "name": "onestopharpshop-api",
  "version": "1.0.0",
  "description": "re-init from Feb 20, 2020",
  "main": "server.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha ./utils/test/*.*",
    "start": "node server.js",
    "startdev": "nodemon server.js"
  },
  "nyc": {
    "exclude": [
      "**/*ForTesting.js"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.3",
    "cheerio-advanced-selectors": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "get-image-colors": "^2.0.0",
    "helmet": "^3.21.3",
    "hpp": "^0.2.3",
    "jimp": "^0.9.5",
    "mocha": "^7.1.1",
    "mongoose": "^5.9.7",
    "nodemon": "^2.0.2",
    "puppeteer": "^2.1.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "serve-favicon": "^2.5.0",
    "slugify": "^1.4.0",
    "ssl-root-cas": "^1.3.1",
    "uuid": "^3.4.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "install": "^0.13.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "max-len": [
        2,
        120,
        4,
        {
          "ignoreUrls": true
        }
      ],
      "no-console": 0,
      "linebreak-style": 0,
      "indent": [
        "error",
        4
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "es6": 0
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "engines": {
    "node": "^10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tmurvvvv@bitbucket.org/tmurvvvv/onestopharpshop-api.git"
  },
  "author": "Tisha Murvihill",
  "license": "ISC",
  "homepage": "https://bitbucket.org/tmurvvvv/onestopharpshop-api#readme"
}
